datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    output   = "app"
}

model ScoreTiming {
    id Int @id @default(autoincrement())

    score Int
    username String

    placement Int @default(0)

    createdOn  DateTime @default(now())
}